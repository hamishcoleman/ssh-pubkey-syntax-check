
# blank lines and lines starting with hash are considered comments

# too few fields
a b

# too many fields
a ssh-dss  b c

# hashed hostname has problem
|1|aaa,zzz ssh-dss  d

# non-standard port number
a,[bb.cc]:z ssh-dss  f
a,[bb.cc ssh-dss  f

# invalid key type
a b c

# Not long enough for length byte
a ssh-rsa AAAAE

